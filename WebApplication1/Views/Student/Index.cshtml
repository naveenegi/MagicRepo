@model List<WebApplication1.Models.Student>
@{
    ViewData["Title"] = "Student List";
}

<div><b>student information</b><br /></div>
@if (Model != null)
{
    <table class="table table-bordered table-responsive table-hover">
        <tr>
            <th>District Id </th>
            <th>Student Id </th>
            <th>First Name </th>
            <th>Last Name </th>
            <th>DateOfBirth</th>
            <th>Ssn</th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.DistrictId</td>
                <td>@item.StudentId</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.DateOfBirth.ToString("{0:MM/dd/yyyy}")</td>
                <td>@item.Ssn</td>


            </tr>

        }
    </table>
}

